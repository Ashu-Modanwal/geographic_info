<package>
  <description brief="geodetic coordinate transforms">

     C++ interfaces for manipulating geodetic coordinates.

  </description>
  <author>Jack O'Quin</author>
  <license>BSD</license>
  <review status="experiemental" notes=""/>
  <url>http://ros.org/wiki/geodesy</url>
  <depend package="angles"/>
  <depend package="geographic_msgs"/>
  <depend package="geometry_msgs"/>
  <depend package="tf"/>

  <export>
    <cpp cflags="-I${prefix}/include"
         lflags="-Wl,-rpath,${prefix}/lib -L${prefix}/lib -lgeoconv"/>
  </export>

</package>


